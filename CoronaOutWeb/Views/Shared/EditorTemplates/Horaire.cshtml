@model ModelesApi.POC.Horaire


    <tr>
        @Html.HiddenFor(model => model.Id, new { @id = "HoraireId" })
        <td>
            @Html.DropDownListFor(model => model.Jour, new SelectList(new JoursSemaine().lJours), "Sélectionnez le jour", new { @class = "form-control" })
        </td>

        <td>
            @Html.EditorFor(model => model.HeureOuverture, @"hh\:mm", new { htmlAttributes = new { @class = "form-control", @id = "timePicker" } })
        </td>

        <td>
            @Html.EditorFor(model => model.HeureFermeture, @"hh\:mm", new { htmlAttributes = new { @class = "form-control", @id = "timePicker" } })
        </td>

    </tr>



